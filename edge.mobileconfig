            <string>Google</string>
            <key>DefaultSearchProviderKeyword</key>
            <string>google.com</string>
            <key>DefaultSearchProviderSearchURL</key>
            <string>https://www.google.com/search?q={searchTerms}</string>
            <key>DefaultSearchProviderSuggestURL</key>
            <string>https://www.google.com/complete/search?client=chrome&amp;q={searchTerms}</string>

