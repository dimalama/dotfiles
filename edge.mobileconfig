            <key>DeveloperToolsAvailability</key>
            <integer>0</integer>

            <!-- Extension Settings - Enable Chrome Web Store -->
            <key>BlockExternalExtensions</key>
            <false/>
            <key>ControlDefaultStateOfAllowExtensionsFromOtherStoresSettingEnabled</key>
            <true/>
            <key>ExtensionInstallSources</key>
            <array>
                <string>https://chrome.google.com/webstore/*</string>
                <string>https://microsoftedge.microsoft.com/addons/*</string>
            </array>
            <key>ExtensionAllowedTypes</key>
            <array>
                <string>extension</string>
                <string>theme</string>
                <string>user_script</string>
            </array>

